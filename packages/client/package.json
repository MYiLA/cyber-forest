{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite serve",
    "build": "yarn run build:clt && yarn run build:srv",
    "build:clt": "vite build --ssrManifest --outDir dist/client",
    "build:srv": "vite build --ssr src/entry-server.tsx --outDir dist/server",

    "start": "cross-env NODE_ENV=production ts-node server.ts",

    "lint": "eslint .",
    "format": "prettier --write .",
    "ts:check": "tsc --skipLibCheck --noEmit --project tsconfig.json",
    "test": "jest ./"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.2",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.0",
    "react-router-dom": "^6.13.0",
    "redux-thunk": "^2.4.2",
    "sass": "^1.63.4",
    "vite-plugin-svgr": "^3.2.0",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.23.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.3.9",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "cross-env": "^7.0.3",
    "typescript": "^4.8.2",
    "vite": "^4.3.9",
    "@types/xss-filters": "^0.0.27"
  },
  "license": "MIT"
}
