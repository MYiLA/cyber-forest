{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite serve",
    "build": "yarn run build:clt && yarn run build:srv",
    "build:clt": "vite build --ssrManifest --outDir dist/client",
    "build:srv": "vite build --ssr src/entry-server.tsx --outDir dist/server && cross-env NODE_ENV=production tsc -p tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "start:local": "cross-env NODE_ENV=production NODE_DEST=local node ./dist/server.js",
    "ssr": "cross-env NODE_ENV=development nodemon server.ts",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "ts:check": "tsc --skipLibCheck --noEmit --project tsconfig.json",
    "test": "jest --verbose ./",
    "sw": "tsc --p tsconfig.sw.json --watch"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.18.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.0",
    "react-router-dom": "^6.13.0",
    "react-toastify": "^9.1.3",
    "redux-thunk": "^2.4.2",
    "sanitize-html": "^2.11.0",
    "sass": "^1.63.4",
    "uuid": "^9.0.0",
    "vite-plugin-svgr": "^3.2.0",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.3.9",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.3.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^28.1.8",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "@types/sanitize-html": "^2.9.0",
    "@types/uuid": "^9.0.2",
    "@types/xss-filters": "^0.0.27",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "canvas": "^2.11.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.3.9",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.16.4"
  },
  "license": "MIT"
}
